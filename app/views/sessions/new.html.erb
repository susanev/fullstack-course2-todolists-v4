<h1>Login</h1>
<%= form_for(:user, url: sessions_path) do |f| %>
	<div class = "field">
		<%= f.label :username %>
		<br />
		<%= f.text_field :username %>
	</div>

	<br />

	<div class = "field">
		<%= f.label :password %>
		<br />
		<%= f.text_field :password %>
	</div>

	<br />

	<div class = "actions">
		<%= f.submit "Login" %>
	</div>
<% end %>

